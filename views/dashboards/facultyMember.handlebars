<!-- ======== HEADER ======== -->
<header id="main-header" class="profile-header py-4 bg-dark text-light">
  <div class="container">
    <!-- ---- HORIZONTAL CARD ---- -->
    <div class="card text-center bg-dark text-light border-0 card-profile-heading">
      <div class="row">
        <!-- Avatar -->
        <div class="col-md-4 d-flex justify-content-center align-items-center">
          {{#if faculty.photo}}
          <img src="/img/facultyMembers/{{faculty.photo}}"
            class="img-fluid avatar--medium avatar--rounded avatar--border-blue" alt="Faculty member photo">
          {{else}}
          <i class="fas fa-user icon-user"></i>
          {{/if}}
        </div>
        <!-- Body -->
        <div class="col-md-8 pl-lg-0 d-flex align-items-center">
          <div class="card-body">
            <h3 class="card-title text-md-left">{{faculty.name}}</h3>
            <h4 class="card-subtitle mb-3 text-md-left">Department Head</h4>
            <h5 class="card-details text-md-left">School of Mathematical Sciences</h5>
            <h5 class="card-details text-md-left">{{faculty.academicRank}}</h5>
            <!-- Social Media -->
            <div class="d-flex justify-content-center justify-content-md-start card-social-media my-4">
              <!-- ==== Disable/enable social media icons ==== -->
              {{#if faculty.facebook}}
              <span class="mr-5">
                <a href="http://www.facebook.com/{{faculty.facebook}}" class="text-light">
                  <i class="fab fa-facebook-f" style="padding-right: .25rem;"></i>
                </a>
              </span>
              {{else}}
              <span class="mr-5">
                <i class="fab fa-facebook-f" style="padding-right: .25rem;"></i>
              </span>
              <script>
                disableElement('.card-social-media .fa-facebook-f');
              </script>
              {{/if}}
              {{#if faculty.twitter}}
              <span class="mr-5">
                <a href="http://twitter.com/{{faculty.twitter}}" class="text-light">
                  <i class="fab fa-twitter" style="padding-right: .25rem;"></i>
                </a>
              </span>
              {{else}}
              <span class="mr-5">
                <i class="fab fa-twitter" style="padding-right: .25rem;"></i>
              </span>
              <script>
                disableElement('.card-social-media .fa-twitter');
              </script>
              {{/if}}
              {{#if faculty.linkedin}}
              <span>
                <a href="http://www.linkedin.com/{{faculty.linkedin}}" class="text-light">
                  <i class="fab fa-linkedin-in" style="padding-right: .25rem;"></i>
                </a>
              </span>
              {{else}}
              <span>
                <i class="fab fa-linkedin-in" style="padding-right: .25rem;"></i>
              </span>
              <script>
                disableElement('.card-social-media .fa-linkedin-in');
              </script>
              {{/if}}
            </div>
            <!-- Email - Tel - Office-->
            <div class="d-flex justify-content-center justify-content-md-start">
              <small class="mr-5  card-email">
                <i class="far fa-envelope " data-toggle="tooltip" data-placement="top" title="email"></i>
                <span class="d-none d-lg-inline">{{faculty.email}}</span>
              </small>
              <small class="mr-5  card-tel">
                <i class="fas fa-phone" data-toggle="tooltip" data-placement="top" title="phone"></i>
                <span class="d-none d-lg-inline">{{faculty.phone}}</span>
              </small>
              <small class="card-office">
                <i class="far fa-building" data-toggle="tooltip" data-placement="top" title="Office Number"></i>
                <span class="d-none d-lg-inline">{{faculty.officeNumber}}</span>
              </small>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</header>

