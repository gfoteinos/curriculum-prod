                <form>
                  <!-- Email -->
                  <div class="form-group mb-4">
                    <label for="email">Email</label>
                    <div class="input-group input-group--white">
                      <div class="input-group-prepend">
                        <span id="addonEmail" class="input-group-text">
                          <i class="fas fa-envelope"></i>
                        </span>
                      </div>
                      <input type="email" name="" id="email" class="form-control form-control-lg" placeholder=""
                        maxlength="64" minlength="7" required="" aria-label="Email" aria-describedby="addonEmail" />
                      <div class="input-group-append">
                        <span class="input-group-text">
                          <a href="#" class="" data-toggle="modal" data-target="#changeEmailModal">
                            <i class="fas fa-pencil-alt"></i>
                          </a>
                        </span>
                      </div>
                    </div>
                  </div>
                  <hr>
                  <!-- Password -->
                  <h4 class="mb-0">Password</h4>
                  <small id="passwordHelpBlock" class="form-text text-muted mb-3">
                    Your password must be 8-20 characters long, contain letters and numbers, and must not contain
                    spaces, special characters, or emoji.
                  </small>
                  <!-- Current Password -->
                  <div class="form-group">
                    <label for="currentPassword">Current Password</label>
                    <div class="input-group input-group--white">
                      <div class="input-group-prepend">
                        <span id="addonCurrentPassword" class="input-group-text">
                          <i class="fas fa-lock"></i>
                        </span>
                      </div>
                      <input type="password" name="" id="currentPassword" class="form-control form-control-lg"
                        placeholder="" maxlength="64" minlength="6" required="" aria-label="Current Password"
                        aria-describedby="addonCurrentPassword" />
                    </div>
                  </div>
                  <!-- New Password -->
                  <div class="form-group">
                    <label for="newPassword">Enter New Password</label>
                    <div class="input-group input-group--white">
                      <div class="input-group-prepend">
                        <span id="addonNewPassword" class="input-group-text">
                          <i class="fas fa-lock"></i>
                        </span>
                      </div>
                      <input type="password" name="" id="newPassword" class="form-control form-control-lg"
                        placeholder="" maxlength="64" minlength="6" required="" aria-label="Enter New Password"
                        aria-describedby="addonNewPassword" />
                    </div>
                  </div>
                  <!-- Confirm New Password -->
                  <div class="form-group">
                    <label for="confirmPassword">Re-type New Password</label>
                    <div class="input-group input-group--white">
                      <div class="input-group-prepend">
                        <span id="addonConfirmPassword" class="input-group-text">
                          <i class="fas fa-lock"></i>
                        </span>
                      </div>
                      <input type="password" name="" id="confirmPassword" class="form-control form-control-lg"
                        placeholder="" maxlength="64" minlength="6" required="" aria-label="Re-type New Password"
                        aria-describedby="addonConfirmPassword" />
                    </div>
                  </div>
                  <div class="d-flex justify-content-end">
                    <button type="button" class="btn btn-primary">
                      Change Password
                    </button>
                  </div>
                </form>
              </div>
