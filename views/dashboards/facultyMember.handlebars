{{!==== Load the profile tab when click "save" profile ====}}

{{!---- In case of form fields error ----}}
{{#equal error_msg 'One or more of the fields "Full Name, Phone, Office, Bio" are empty. Please fill in the fields where missing.' }}
{{!-- Load Profile Form --}}
<script>profileClick()</script>
{{/equal}}

{{!---- In case of error uploading photo ----}}
{{#equal error_msg 'Error uploading photo: File too large'  }}
{{!-- Load Profile Form --}}
<script>profileClick()</script>
{{/equal}}

{{!---- In case of success ----}}
{{#equal success_msg 'Profile updated.' }}
{{!-- Load Profile Form --}}
<script>profileClick()</script>
{{/equal}}

<!-- ======== HEADER ======== -->
<header id="main-header" class="profile-header py-4 bg-dark text-light">
  <div class="container">
    <div class="card text-center bg-dark text-light border-0 card-profile-heading">
      <div class="row">
        <!-- AVATAR -->
        <div class="col-md-4 d-flex justify-content-center align-items-center">
          {{#if faculty.photo}}
          <img src="/img/facultyMembers/{{faculty.photo}}"
            class="img-fluid avatar--medium avatar--rounded avatar--border-blue" alt="Faculty member photo">
          {{else}}
          <i class="fas fa-user icon-user"></i>
          {{/if}}
        </div>
        <!-- BODY -->
        <div class="col-md-8 pl-lg-0 d-flex align-items-center">
          <div class="card-body">
            <h3 class="card-title text-md-left">{{faculty.name}}</h3>
            <h4 class="card-subtitle mb-3 text-md-left">Department Head</h4>
            <h5 class="card-details text-md-left">School of Mathematical Sciences</h5>
            <h5 class="card-details text-md-left">Associate Professor</h5>
            <!-- Social Media -->
            <div class="d-flex justify-content-center justify-content-md-start card-social-media my-4">
              <!-- ==== Disable/enable social media icons ==== -->
              {{#if faculty.facebook}}
              <span class="mr-5">
                <a href="http://www.facebook.com/{{faculty.facebook}}" class="text-light">
                  <i class="fab fa-facebook-f" style="padding-right: .25rem;"></i>
                </a>
              </span>
              {{else}}
              <span class="mr-5">
                <i class="fab fa-facebook-f" style="padding-right: .25rem;"></i>
              </span>
              <script>
                disableElement('.card-social-media .fa-facebook-f');
              </script>
              {{/if}}
              {{#if faculty.twitter}}
              <span class="mr-5">
                <a href="http://twitter.com/{{faculty.twitter}}" class="text-light">
                  <i class="fab fa-twitter" style="padding-right: .25rem;"></i>
                </a>
              </span>
              {{else}}
              <span class="mr-5">
                <i class="fab fa-twitter" style="padding-right: .25rem;"></i>
              </span>
              <script>
                disableElement('.card-social-media .fa-twitter');
              </script>
              {{/if}}
              {{#if faculty.linkedin}}
              <span>
                <a href="http://www.linkedin.com/{{faculty.linkedin}}" class="text-light">
                  <i class="fab fa-linkedin-in" style="padding-right: .25rem;"></i>
                </a>
              </span>
              {{else}}
              <span>
                <i class="fab fa-linkedin-in" style="padding-right: .25rem;"></i>
              </span>
              <script>
                disableElement('.card-social-media .fa-linkedin-in');
              </script>
              {{/if}}
            </div>
            <!-- Email - Tel - Office-->
            <div class="d-flex justify-content-center justify-content-md-start">
              <small class="mr-5  card-email">
                <i class="far fa-envelope " data-toggle="tooltip" data-placement="top" title="email"></i>
                <span class="d-none d-lg-inline">{{faculty.email}}</span>
              </small>
              <small class="mr-5  card-tel">
                <i class="fas fa-phone" data-toggle="tooltip" data-placement="top" title="phone"></i>
                <span class="d-none d-lg-inline">{{faculty.phone}}</span>
              </small>
              <small class="card-office">
                <i class="far fa-building" data-toggle="tooltip" data-placement="top" title="Office Number"></i>
                <span class="d-none d-lg-inline">{{faculty.officeNumber}}</span>
              </small>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</header>

<!-- ======== BODY ======== -->
<section id="facultyDashboard">
  <div class="container py-5 bg-white">
    <div class="row">
      <!-- NAVIGATION LIST -->
      <div class="col-12 col-lg-4 mb-3">
        <div class="list-group" id="list-tab" role="tablist">
          <a class="list-group-item list-group-item-action active" id="account-list" data-toggle="list" href="#account"
            role="tab" aria-controls="account">Account</a>
          <a class="list-group-item list-group-item-action" id="profile-list" data-toggle="list" href="#profile"
            role="tab" aria-controls="profile">Profile</a>
          <a class="list-group-item list-group-item-action" id="module-list" data-toggle="list" href="#modules"
            role="tab" aria-controls="modules">Modules</a>
        </div>
        {{> _msg}}
      </div>
      <!-- WORK AREA -->
      <div class="col-12 col-lg-8 pl-lg-0 mb-3">
        <div class="tab-content" id="nav-tabContent">
          <!-- Account Settings-->
          <div class="tab-pane active" id="account" role="tabpanel" aria-labelledby="account-list">
            <div class="card card-manage-account">
              <div class="card-header">
                <h3 class="mb-0">Account Info</h3>
                <small class="">
                  Edit your account settings and change your password here
                </small>
              </div>
              <div class="card-body">
                <form action="/dashboards/facultyMember/email/{{faculty.id}}?_method=PUT" method="post">
                  <input type="hidden" name="_method" value="PUT">
                  <!-- Email -->
                  <div class="form-group mb-4">
                    <label for="email">Email</label>
                    <div class="input-group input-group--white">
                      <div class="input-group-prepend">
                        <span id="addonEmail" class="input-group-text">
                          <i class="fas fa-envelope"></i>
                        </span>
                      </div>
                      <input type="email" name="email" id="email" class="form-control form-control-lg"
                        placeholder="{{faculty.email}}" maxlength="64" minlength="7" required="" aria-label="Email"
                        aria-describedby="addonEmail" />
                      <div class="input-group-append">
                        <button type="submit" class="btn btn-outline-success button--border-grey-light">
                          <i class="fas fa-save"></i>
                        </button>
                      </div>
                    </div>
                  </div>
                </form>
                <form action="/dashboards/facultyMember/password/{{faculty.id}}?_method=PUT" method="post">
                  <input type="hidden" name="_method" value="PUT">
                  <hr>
                  <!-- Password -->
                  <h4 class="mb-0">Password</h4>
                  <small class="form-text text-muted mb-3">
                    Your password must be 8-20 characters long, contain letters and numbers, and must not contain
                    spaces, special characters, or emoji.
                  </small>
                  <!-- Current Password -->
                  <div class="form-group">
                    <label for="currentPassword">Current Password</label>
                    <div class="input-group input-group--white">
                      <div class="input-group-prepend">
                        <span id="addonCurrentPassword" class="input-group-text">
                          <i class="fas fa-lock"></i>
                        </span>
                      </div>
                      <input type="password" name="password" id="currentPassword" class="form-control form-control-lg"
                        placeholder="" maxlength="64" minlength="4" aria-label="Current Password"
                        aria-describedby="addonCurrentPassword" required />
                    </div>
                  </div>
                  <!-- New Password -->
                  <div class="form-group">
                    <label for="newPassword">Enter New Password</label>
                    <div class="input-group input-group--white">
                      <div class="input-group-prepend">
                        <span id="addonNewPassword" class="input-group-text">
                          <i class="fas fa-lock"></i>
                        </span>
                      </div>
                      <input type="password" name="password1" id="newPassword" class="form-control form-control-lg"
                        placeholder="" maxlength="64" minlength="4" aria-label="Enter New Password"
                        aria-describedby="addonNewPassword" required />
                    </div>
                  </div>
                  <!-- Confirm New Password -->
                  <div class="form-group">
                    <label for="confirmPassword">Re-type New Password</label>
                    <div class="input-group input-group--white">
                      <div class="input-group-prepend">
                        <span id="addonConfirmPassword" class="input-group-text">
                          <i class="fas fa-lock"></i>
                        </span>
                      </div>
                      <input type="password" name="password2" id="confirmPassword" class="form-control form-control-lg"
                        placeholder="" maxlength="64" minlength="4" aria-label="Re-type New Password"
                        aria-describedby="addonConfirmPassword" required />
                    </div>
                  </div>
                  <!-- Save button -->
                  <div class="d-flex justify-content-end">
                    <button type="submit" class="btn btn-success">
                      Change Password
                    </button>
                  </div>
                </form>
              </div>
            </div>
          </div>
          <!-- Profile Settings-->
          <div class="tab-pane" id="profile" role="tabpanel" aria-labelledby="profile-list">
            <div class="card card-manage-profile">
              <div class="card-header">
                <h3 class="mb-0">Profile</h3>
                <small class="">Add Information About Yourself</small>
              </div>
              <div class="card-body">
                <form action="/dashboards/facultyMember/profile/{{faculty.id}}?_method=PUT" method="post"
                  enctype="multipart/form-data">
                  <input type="hidden" name="_method" value="PUT">
                  <!-- ==== BASICS ==== -->
                  <h4>Basics</h4>
                  <!-- Full Name -->
                  <div class="form-group">
                    <label for="fullName">Full Name</label>
                    <div class="input-group input-group--white">
                      <div class="input-group-prepend">
                        <span class="input-group-text" aria-label="Input a full name">
                          <i class="fas fa-user"></i>
                        </span>
                      </div>
                      <input type="text" name="fullName" id="fullName" class="form-control form-control-lg"
                        placeholder="" maxlength="64" value="{{faculty.name}}" required />
                    </div>
                  </div>
                  <div class="form-row">
                    <!-- Phone -->
                    <div class="form-group col-md-6">
                      <label for="phone">Phone</label>
                      <div class="input-group input-group--white mb-1">
                        <div class="input-group-prepend">
                          <span class="input-group-text" aria-label="Input a phone number">
                            <i class="fas fa-phone"></i>
                          </span>
                        </div>
                        <input type="tel" name="phone" id="phone" class="form-control form-control-lg"
                          pattern="[0]{1}-[0-9]{4}-[0-9]{6}" aria-describedby="phoneHelp" value="{{faculty.phone}}" />
                      </div>
                      <small id="phoneHelp" class="form-text text-muted">
                        Format: 0-1234-567890
                      </small>
                    </div>
                    <!-- Office Number -->
                    <div class="form-group col-md-6">
                      <label for="office">Office</label>
                      <div class="input-group input-group--white mb-1">
                        <div class="input-group-prepend">
                          <span class="input-group-text" aria-label="Input an office number">
                            <i class="far fa-building"></i>
                          </span>
                        </div>
                        <input type="text" name="officeNumber" id="office" class="form-control form-control-lg"
                          pattern="[A-Z]{2}[0-9]{2}[A-Z]{1}" aria-describedby="officeNumHelp"
                          value="{{faculty.officeNumber}}" required />
                      </div>
                      <small id="officeNumHelp" class="form-text text-muted">
                        Format: AB12D
                      </small>
                    </div>
                  </div>
                  {{#if faculty.photo}}
                  <hr class="hr--y-wide">
                  <div class="form-group">
                    <div class="d-flex flex-row justify-content-center">
                      <div>
                        <img src="/img/facultyMembers/{{faculty.photo}}"
                          class="img-fluid avatar--pico avatar--rounded avatar--border-blue-thin mr-3"
                          alt="Faculty member photo">
                      </div>
                      <div class="align-self-center">
                        <div class="form-group mb-0">
                          <div class="custom-control custom-checkbox">
                            <input type="checkbox" class="custom-control-input" id="deletePhoto" name="deletePhoto">
                            <label class="custom-control-label" for="deletePhoto">Delete profile photo</label>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>

                  {{!-- <img src="/img/facultyMembers/{{faculty.photo}}"
                    class="img-fluid avatar--pico avatar--rounded avatar--border-blue" alt="Faculty member photo">
                  <div class="form-group form-check">
                    <input type="checkbox" class="form-check-input" id="deletePhoto">
                    <label class="form-check-label" for="deletePhoto">Delete profile photo</label>
                  </div> --}}
                  {{else}}
                  <!-- Upload CV file -->
                  <div class="form-group">
                    <label for="photo">Photo</label>
                    <small><span class="text-danger">(Optional)</span></small>
                    <!-- Custom File Input -->
                    <div class="custom-file">
                      <input type="file" name="photo" id="photo" class="custom-file-input">
                      <label class="custom-file-label" for="photo">Choose your profile image</label>
                      <small class="form-text text-muted" id="fileHelp">Max 3mb size image file</small>
                    </div>
                  </div>
                  {{/if}}
                  <hr class="hr--y-wide">
                  <!-- ==== BIO ==== -->
                  <h4 id="bio-label">Bio</h4>
                  <div class="form-group">
                    <small>Write a few words about youself</small>
                    <textarea id="bio" name="bio" class="form-control" aria-labelledby="bio-label" required>
                      {{faculty.bio}} 
                    </textarea>
                  </div>
                  <hr class="hr--y-wide">
                  <!-- ==== SOCIAL MEDIA ==== -->
                  <h4>Social Media <small><span class="text-danger">(Optional)</span></small></h4>
                  <!-- Facebook -->
                  <div class="form-group">
                    <label for="facebook">Facebook Profile</label>
                    <div class="input-group">
                      <div class="input-group-prepend">
                        <span class="input-group-text">
                          <span class="d-sm-none" aria-label="Input your Facebook username">
                            <i class="fab fa-facebook-f"></i>
                          </span>
                          <span class="d-none d-sm-block">
                            http://www.facebook.com/
                          </span>
                        </span>
                      </div>
                      <input type="text" name="facebook" id="facebook" class="form-control form-control-lg"
                        aria-describedby="facebookHelp" value="{{faculty.facebook}}" />
                    </div>
                    <small id="facebookHelp" class="form-text text-muted">
                      Input your Facebook username (e.g. johndoe).
                    </small>
                  </div>
                  <!-- Twitter -->
                  <div class="form-group">
                    <label for="twitter">Twitter Profile</label>
                    <div class="input-group">
                      <div class="input-group-prepend">
                        <span class="input-group-text">
                          <span class="d-sm-none" aria-label="Input your Twitter username">
                            <i class="fab fa-twitter"></i>
                          </span>
                          <span class="d-none d-sm-block">
                            http://twitter.com/
                          </span>
                        </span>
                      </div>
                      <input type="text" name="twitter" id="twitter" class="form-control form-control-lg"
                        aria-describedby="twitterHelp" value="{{faculty.twitter}}" />
                    </div>
                    <small id="twitterHelp" class="form-text text-muted">
                      Add your Twitter username (e.g. johndoe).
                    </small>
                  </div>
                  <!-- Linkedin -->
                  <div class="form-group">
                    <label for="linkedin">Linkedin Profile</label>
                    <div class="input-group">
                      <div class="input-group-prepend">
                        <span class="input-group-text">
                          <span class="d-sm-none" aria-label="Input your Linkedin username">
                            <i class="fab fa-linkedin-in"></i>
                          </span>
                          <span class="d-none d-sm-block">
                            http://www.linkedin.com/
                          </span>
                        </span>
                      </div>
                      <input type="text" name="linkedin" id="linkedin" class="form-control form-control-lg"
                        aria-describedby="linkedinHelp" value="{{faculty.linkedin}}" />
                    </div>
                    <small id="linkedinHelp" class="form-text text-muted mb-3">
                      Input your LinkedIn resource id (e.g. in/johndoe).
                    </small>
                  </div>
                  <!-- Save Button -->
                  <div class="d-flex justify-content-end">
                    <button type="submit" class="btn btn-success">
                      Save
                    </button>
                  </div>
                </form>
              </div>
            </div>
          </div>
